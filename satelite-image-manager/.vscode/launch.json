{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "name": "ng serve",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Start with debugging",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "preLaunchTask": "ngserve",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {},
      "runtimeArgs": ["--remote-debugging-port=9222"]
    }
  ]
}
